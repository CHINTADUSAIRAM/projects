#include <stdio.h>
int main() {
   int y;
   printf("Enter any year: ");
   scanf("%d", &y);

   if (y % 400 == 0)
    {
      printf("%d is a leap year.", y);
   }
   else if (y % 100 == 0) 
   {
      printf("%d is not a leap year.", y);
   }
   else if (y % 4 == 0) 
   {
      printf("%d is a leap year.", y);
   }
   else
    {
      printf("%d is not a leap year.", y);
   }

   return 0;
}

output
case 1:
Enter any year: 2020
2020 is a leap year.

case 2:
Enter any year: 2022
2022 is not a leap year.
